<!-- will display the table of users -->


<h1 class="fst-italic lh-1 mb-4 text-white">&nbsp;&nbsp;Account Management</h1>
<br><br>
<table style="width: 100%; background-color: antiquewhite; border: 1px solid black;">
  <thead style="background-color: rgba(0, 0, 0, 0.85);">
    <tr>
      <th style="text-align: left;">Name</th>
      <th style="text-align: left;">Email</th>
      <th style="text-align: left;">Username</th>
      <th style="text-align: left;">Password</th>
      <th style="text-align: left;">Super User</th>
    </tr>
  </thead>
  <tbody style="font-family: Arial; font-size: 9pt;">
       <tr>
           <td><%= user.name %></td>
           <td><%= user.email %></td>
           <td><%= user.username %></td>
           <td><%= user.password %></td>
           <td><%= user.super_user %></td>
           <td style="text-align: center;">
            </td>
            <td style="text-align: center;">
      </tr>
    </tbody>

    <a class="btn btn-primary" href="/edit">update</a>
    <a class="btn btn-primary" href="/delete">delete</a>